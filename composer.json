{
    "name": "idxos/portfolio",
    "description": "Portfolio site built on OctoberCMS PHP framework.  Custom composer repo my basic dev tools.",
    "homepage": "http://lknpro.com",
    "keywords": ["october", "cms", "octobercms", "laravel"],
    "authors": [
        {
            "name": "Christopher J. Roberts",
            "email": "croberts@lknpro.com",
            "role": "Ninja"
        }
    ],
    "support": {
        "issues": "https://github.com/octobercms/october/issues",
        "forum": "http://octobercms.com/forum/",
        "docs": "http://octobercms.com/docs/",
        "irc": "irc://irc.freenode.net/october",
        "source": "https://github.com/idxos/portfolio"
    },
    "require": {
        "php": ">=5.4",
        "october/rain": "~1.0",
        "october/system": "~1.0",
        "october/backend": "^1.0@dev",
        "october/cms": "~1.0",
        "laravel/framework": "5.0.*",
        "illuminate/html": "5.0.*",
        "october/october": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/phpunit-selenium": ">=1.2"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/UiTestCase.php"
        ]
    },
    "scripts": {
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
